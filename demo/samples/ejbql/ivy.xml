<ivy-module version="2.0">
	<info organisation="net.sf.jasperreports" module="jasperreports">
		<extends organisation="net.sf.jasperreports" module="jasperreports" revision="*" location="../../../ivy.xml"/>
	</info>
	<dependencies>
		<dependency org="jakarta.persistence" name="jakarta.persistence-api" rev="3.1.0"/>
		<dependency org="org.hibernate.orm" name="hibernate-core" rev="6.3.1.Final" conf="test->*"/>
		<dependency org="jakarta.transaction" name="jakarta.transaction-api" rev="2.0.1" conf="test->*"/>
		<dependency org="org.jboss.logging" name="jboss-logging" rev="3.5.0.Final" conf="test->*"/>
		<dependency org="org.hibernate.common" name="hibernate-commons-annotations" rev="6.0.6.Final" conf="test->*"/>
		<dependency org="io.smallrye" name="jandex" rev="3.1.5" conf="test->*"/>
		<dependency org="com.fasterxml" name="classmate" rev="1.5.1" conf="test->*"/>
		<dependency org="net.bytebuddy" name="byte-buddy" rev="1.14.7" conf="test->*"/>
		<dependency org="org.antlr" name="antlr4-runtime" rev="4.10.1" conf="test->*"/>
		<dependency org="com.fasterxml.jackson.module" name="jackson-module-jakarta-xmlbind-annotations" rev="2.14.1" conf="test->*"/>
		<dependency org="org.hsqldb" name="hsqldb" rev="2.7.1" conf="test->*"/>
		<!-- JAXB API got removed from Java 11 -->
		<dependency org="jakarta.xml.bind" name="jakarta.xml.bind-api" rev="4.0.1" conf="test->*"/>
		<exclude org="com.fasterxml.jackson.module" artifact="jackson-module-jaxb-annotations"/>
   	</dependencies>
</ivy-module>
